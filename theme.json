{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
  "customTemplates": [
		{
			"title": "No title",
			"name": "no-title",
			"postTypes": [
				"page"
			]
		}
	],
	"settings": {
		"useRootPaddingAwareAlignments": true,
		"color": {
			"background": true,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"link": false,
			"palette": [
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#36C5F0"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#2EB67D"
				},
				{
					"name": "Light",
					"slug": "light",
					"color": "#fff"
				},
				{
					"name": "Dark",
					"slug": "dark",
					"color": "#000"
				}
			],
			"text": true
		},
		"layout": {
			"contentSize": "1280px",
			"wideSize": "1536px"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"spacingSizes": [
				{
					"name": "Small",
					"slug": "sm",
					"size": "12px"
				},
				{
					"name": "Medium",
					"slug": "md",
					"size": "24px"
				},
				{
					"name": "Large",
					"slug": "lg",
					"size": "36px"
				},
				{
					"name": "XL",
					"slug": "xl",
					"size": "48px"
				},
				{
					"name": "2XL",
					"slug": "2xl",
					"size": "60px"
				},
				{
					"name": "3XL",
					"slug": "3xl",
					"size": "100px"
				},
				{
					"name": "4XL",
					"slug": "4xl",
					"size": "160px"
				}
			]
		},
		"typography": {
			"customFontSize": false,
			"fontStyle": false,
			"fontWeight": true,
			"fluid": true,
			"letterSpacing": true,
			"lineHeight": true,
			"textDecoration": false,
			"textTransform": true,
			"dropCap": false,
			"fontSizes": [
				{
					"name": "Small",
					"slug": "sm",
					"size": "14px",
					"fluid": {
						"min": "14px",
						"max": "16px"
					}
				},
				{
					"name": "Medium",
					"slug": "md",
					"size": "18px",
					"fluid": {
						"min": "18px",
						"max": "22px"
					}
				},
				{
					"name": "Large",
					"slug": "lg",
					"size": "18px",
					"fluid": {
						"min": "18px",
						"max": "24px"
					}
				},
				{
					"name": "XL",
					"slug": "xl",
					"size": "20px",
					"fluid": {
						"min": "20px",
						"max": "26px"
					}
				},
				{
					"name": "2XL",
					"slug": "2-xl",
					"size": "22px",
					"fluid": {
						"min": "22px",
						"max": "28px"
					}
				},
				{
					"name": "3XL",
					"slug": "3-xl",
					"size": "28px",
					"fluid": {
						"min": "28px",
						"max": "36px"
					}
				},
				{
					"name": "4XL",
					"slug": "4-xl",
					"size": "30px",
					"fluid": {
						"min": "30px",
						"max": "51px"
					}
				}
			],
			"fontFamilies": [
				{
					"name": "Sans",
					"slug": "sans",
					"fontFamily": "ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, \"Noto Sans\", sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\""
				},
				{
					"name": "Serif",
					"slug": "serif",
					"fontFamily": "ui-serif, Georgia, Cambria, \"Times New Roman\", Times, serif"
				}
			]
		},
		"blocks": {}
	},
	"styles": {
		"spacing": {
			"padding": {
				"right": "24px",
				"left": "24px"
			}
		}
	}
}
