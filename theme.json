{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "loop",
			"name": "loop",
			"title": "Loop"
		}
	],
	"settings": {
		"blocks": {
			"core/group": {
				"color": {
					"background": true
				}
			}
		},
		"custom": {
			"color": {
				"primary": "#DD93B5",
				"secondary": "#F47733",
				"tertiary": "#0F5B66",
				"error": "#d4351c",
				"success": "#00703c",
				"focus": "#fd0",
				"base": "#fff",
				"contrast": "#000",
				"neutral": "#f2f2f2"
			},
			"layout": {
				"content": "1280px",
				"wide": "1536px",
				"padding": "var(--wp--preset--spacing--xl)",
				"gap": "var(--wp--preset--spacing--2-xl)"
			},
			"lineHeight": {
				"none": "1",
				"tight": "1.25",
				"snug": "1.375",
				"normal": "1.5",
				"relaxed": "1.625",
				"loose": "2"
			},
			"transitionDuration": {
				"quick": "100ms",
				"base": "250ms",
				"slow": "600ms"
			}
		},
		"color": {
			"background": false,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"link": false,
			"palette": [
				{
					"name": "Primary",
					"slug": "primary",
					"color": "var(--wp--custom--color--primary)"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "var(--wp--custom--color--secondary)"
				},
				{
					"name": "Tertiary",
					"slug": "tertiary",
					"color": "var(--wp--custom--color--tertiary)"
				}
			],
			"text": false
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"spacingSizes": [
				{
					"name": "xs",
					"size": "4px",
					"slug": "xs"
				},
				{
					"name": "sm",
					"size": "8px",
					"slug": "sm"
				},
				{
					"name": "md",
					"size": "12px",
					"slug": "md"
				},
				{
					"name": "lg",
					"size": "16px",
					"slug": "lg"
				},
				{
					"name": "xl",
					"size": "24px",
					"slug": "xl"
				},
				{
					"name": "2xl",
					"size": "clamp(24px, 4.166vw, 32px)",
					"slug": "2-xl"
				},
				{
					"name": "3xl",
					"size": "clamp(32px, 6.25vw, 48px)",
					"slug": "3-xl"
				},
				{
					"name": "4xl",
					"size": "clamp(48px, 8.333vw, 72px)",
					"slug": "4-xl"
				},
				{
					"name": "5xl",
					"size": "clamp(72px, 12.5vw, 112px)",
					"slug": "5-xl"
				}
			],
			"units": ["%", "px", "rem", "vh", "vw"]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"name": "Sans",
					"slug": "sans",
					"fontFamily": "ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, \"Noto Sans\", sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\""
				},
				{
					"name": "Serif",
					"slug": "serif",
					"fontFamily": "ui-serif, Georgia, Cambria, \"Times New Roman\", Times, serif"
				}
			],
			"fontSizes": [
				{
					"name": "xs",
					"slug": "xs",
					"size": "14px",
					"fluid": false
				},
				{
					"name": "Small",
					"slug": "sm",
					"size": "16px",
					"fluid": false
				},
				{
					"name": "Base",
					"slug": "base",
					"size": "18px",
					"fluid": {
						"min": "16px",
						"max": "18px"
					}
				},
				{
					"name": "Medium",
					"slug": "md",
					"size": "20px",
					"fluid": {
						"min": "18px",
						"max": "20px"
					}
				},
				{
					"name": "Large",
					"slug": "lg",
					"size": "24px",
					"fluid": {
						"min": "20px",
						"max": "24px"
					}
				},
				{
					"name": "xl",
					"slug": "xl",
					"size": "34px",
					"fluid": {
						"min": "28px",
						"max": "34px"
					}
				},
				{
					"name": "2xl",
					"slug": "2-xl",
					"size": "60px",
					"fluid": {
						"min": "42px",
						"max": "60px"
					}
				},
				{
					"name": "3xl",
					"slug": "3-xl",
					"size": "96px",
					"fluid": {
						"min": "60px",
						"max": "96px"
					}
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/post-content": {
				"css": "& * + :is( h1, h2, h3, h4, h5, h6 ) {margin-block-start: var(--wp--preset--spacing--3-xl);}"
			},
			"core/group": {
				"css": "&:where(.has-background) + :where(.has-background) {margin-block-start: 0;}"
			},
			"core/heading": {
				"css": "& + * {margin-block-start: var(--wp--preset--spacing--md);}"
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--layout--gap)",
			"padding": {
				"right": "var(--wp--custom--layout--padding)",
				"left": "var(--wp--custom--layout--padding)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "var(--wp--custom--line-height--relaxed)"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--line-height--tight)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2-xl)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--lg)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--md)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--sm)"
				}
			},
			"link": {
				"typography": {
					"textDecoration": "underline"
				}
			}
		}
	}
}
